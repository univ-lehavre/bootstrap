{
	"name": "@univ-lehavre/bootstrap",
	"version": "0.0.3",
	"license": "UNLICENSED",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/univ-lehavre/bootstrap.git"
	},
	"bugs": {
		"url": "https://github.com/univ-lehavre/bootstrap/issues"
	},
	"homepage": "https://github.com/univ-lehavre/bootstrap#readme",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build && npm run prepack",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"prepack": "svelte-kit sync && svelte-package && publint",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"test:unit": "vitest",
		"test": "npm run test:unit -- --run && npm run test:e2e",
		"test:e2e": "playwright test",
		"storybook": "storybook dev -p 6006",
		"build-storybook": "storybook build",
		"changeset": "changeset",
		"changeset-version": "changeset version"
	},
	"files": [
		"dist",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*"
	],
	"sideEffects": [
		"**/*.css"
	],
	"svelte": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"type": "module",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js"
		}
	},
	"peerDependencies": {
		"svelte": "^5.0.0"
	},
	"devDependencies": {
		"@changesets/cli": "^2.28.1",
		"@chromatic-com/storybook": "^3.2.6",
		"@eslint/compat": "^1.2.7",
		"@eslint/js": "^9.22.0",
		"@playwright/test": "^1.51.1",
		"@storybook/addon-essentials": "^8.6.7",
		"@storybook/addon-svelte-csf": "5.0.0-next.28",
		"@storybook/blocks": "^8.6.7",
		"@storybook/experimental-addon-test": "^8.6.7",
		"@storybook/svelte": "^8.6.7",
		"@storybook/sveltekit": "^8.6.7",
		"@storybook/test": "^8.6.7",
		"@sveltejs/adapter-node": "^5.2.12",
		"@sveltejs/kit": "^2.20.0",
		"@sveltejs/package": "^2.3.10",
		"@sveltejs/vite-plugin-svelte": "^5.0.3",
		"@testing-library/jest-dom": "^6.6.3",
		"@testing-library/svelte": "^5.2.7",
		"@types/node": "^22.13.10",
		"@vitest/browser": "3.0.9",
		"@vitest/coverage-v8": "3.0.9",
		"eslint": "^9.22.0",
		"eslint-config-prettier": "^10.1.1",
		"eslint-plugin-svelte": "^3.3.1",
		"globals": "^16.0.0",
		"jsdom": "^26.0.0",
		"playwright": "^1.51.1",
		"prettier": "^3.5.3",
		"prettier-plugin-svelte": "^3.3.3",
		"publint": "^0.3.9",
		"storybook": "^8.6.7",
		"svelte": "^5.23.1",
		"svelte-check": "^4.1.5",
		"typescript": "^5.8.2",
		"typescript-eslint": "^8.26.1",
		"vite": "^6.2.2",
		"vitest": "^3.0.9"
	},
	"keywords": [
		"svelte"
	],
	"pnpm": {
		"onlyBuiltDependencies": [
			"esbuild"
		]
	}
}
